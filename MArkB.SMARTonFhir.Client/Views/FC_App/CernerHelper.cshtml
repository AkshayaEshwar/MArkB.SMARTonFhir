@model MArkB.SMARTonFhir.Cerner.Models.CernerConnectionInformation

@{
  ViewBag.Title = "Cerner Connection Information";
  Layout = "~/Views/Shared/_LayoutSMART.cshtml";
}
<h2>@ViewBag.Title.</h2>
