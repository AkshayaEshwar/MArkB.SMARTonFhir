        @Html.TextBoxFor(m => m.AuthorizeUrl, new { @class = "form-control" })
    </div>
    @Html.LabelFor(m => m.ServiceIdentifier, new { @class = "col-md-4 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.ServiceIdentifier, new { @class = "form-control" })
    </div>
    @Html.LabelFor(m => m.RedirectURL, new { @class = "col-md-4 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.RedirectURL, new { @class = "form-control" })
    </div>

    @Html.LabelFor(m => m.ErrorCorrelationId, new { @class = "col-md-4 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.ErrorCorrelationId, new { @class = "form-control" })
    </div>

    @Html.LabelFor(m => m.ErrorDescription, new { @class = "col-md-4 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.ErrorDescription, new { @class = "form-control" })
    </div>

</div>

